{
    "manifest_version": 3,
    "name": "Email Contact Extractor",
    "version": "1.0",
    "description": "Extract contact information from email content",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://mail.google.com/*"],
        "js": ["contentScript.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  