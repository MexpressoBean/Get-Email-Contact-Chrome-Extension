{
  "manifest_version": 3,
  "name": "AI Contact Fetcher: Gmail to Google Contacts",
  "version": "1.0",
  "description": "Easily extract contact information from Gmail messages using AI and create Google Contacts with a single click!",
  "permissions": ["activeTab", "scripting", "tabs", "identity", "storage"],

  "action": {
    "default_popup": "src/popup/popup.html"
  },
  "background": {
    "service_worker": "src/background/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["src/content/contentScript.js"]
    }
  ],
  "oauth2": {
    "client_id": "1032215786471-8v4ht119pk7nuvramcv7iqiug25saoub.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/contacts"]
  },
  "key": ""
}
