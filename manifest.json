{
  "manifest_version": 3,
  "name": "Email Contact Extractor",
  "version": "1.0",
  "description": "Extract contact information from email content",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "identity",
    "storage"
  ],

  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "oauth2": {
    "client_id": "1032215786471-8v4ht119pk7nuvramcv7iqiug25saoub.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/contacts"]
  },
  "key": ""
}
